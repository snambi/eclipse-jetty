<?xml version="1.0" encoding="UTF-8"?>
<document xmlns="http://maven.apache.org/changes/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/changes/1.0.0 http://maven.apache.org/xsd/changes-1.0.0.xsd">

	<properties>
		<title>Eclipse Jetty Changes</title>
		<author>Christian Köberl</author>
	</properties>

	<body>
		<release version="3.4.0-beta-1" date="2011-06-01">
			<action dev="ckoeberl" type="add">
				Jetty 7 compatability.
			</action>
			<action dev="thred" type="add">
				Enable JSP Support: JSP support and version can be selected in the run configuration.
      		</action>
      		<action dev="thred" type="add">
      			Excluded libraries can now be edited - default value is &quot;.*servlet-api.*, .*selenium-server.*standalone\.jar&quot;
      		</action>
		</release>

		<release version="3.4.0-alpha-2" date="2009-03-06">
			<action dev="ckoeberl" type="update">
				Jetty 5 and 6 compatability.
			</action>
		</release>

		<release version="3.4.0-alpha-1" date="2009-02-23"
			description="Initial release">
		</release>
	</body>
</document>